package operadoresExemplo;

import java.math.*;

public class operadoresExemploCalculo {

	public static void main(String[] args) {
		// x^2 + 12x -13 = 0
		// 2x^2 - 16x -18 = 0
		
		double a, b, c;
		
		a = 1;
		b = 12;
		c = -13;
		
		double delta = 0;
		
		delta = (b * b) - (4 * a * c);
		
		System.out.printf("O valor do delta é: %.2f\n", delta);
		
		double x_linha = 0;
		x_linha = (-b + Math.sqrt(delta)) / (2*a);

	}

}
